a = 0
b = 1
c = 0
lista = []
while a < 9999:
    lista.append(a)
    lista.append(b)
    c = b + a
    lista.append(c)
    a = b
    b = c
num = int(input('Digite um número, verificaremos se está na sequência de Fibonacci: '))
print(lista)
if num == lista:
    print('O número está na sequência!')
else:
    print('O número não está na sequência!')
